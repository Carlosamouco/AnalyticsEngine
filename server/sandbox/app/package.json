{
  "name": "docker-sandbox",
  "version": "0.1.0",
  "description": "Sandbox container",
  "private": true,
  "author": "Carlos Samouco",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/index.js",
    "watch-node": "nodemon --watch src dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/index.js --legacy-watch",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "build-webpack": "webpack --optimize-minimize --define process.env.NODE_ENV=\"'production'\""
  },
  "dependencies": {
    "archiver": "^2.1.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.3",
    "multer": "^1.3.0"
  },
  "devDependencies": {
    "@types/archiver": "^2.1.1",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.16.8",
    "@types/express": "^4.11.1",
    "@types/multer": "^1.3.6",
    "@types/node": "^7.0.56",
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.2",
    "ts-loader": "^4.2.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2",
    "uglifyjs": "^2.4.11",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14"
  }
}
