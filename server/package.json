{
  "name": "express-typescript-starter",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
  },
  "author": "Bowden Kelly",
  "license": "MIT",
  "scripts": {
    "start": "npm run build && npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/server.js",
    "watch-node": "nodemon --ignore uploads --ignore sandbox/app/dist --ignore temp --ignore sandbox/app/logs --exec 'docker rm -f $(docker ps -a -q --filter ancestor=docker-sandbox); node dist/server.js' --legacy-watch",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js --legacy-watch",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "dependencies": {
    "async": "^2.6.0",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "connect-mongo": "^2.0.0",
    "csv-streamify": "^4.0.0",
    "decompress": "^4.2.0",
    "dockerode": "^2.5.5",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-validator": "^4.3.0",
    "fbgraph": "^1.4.1",
    "form-data": "^2.3.2",
    "js2xmlparser": "^3.0.0",
    "lodash": "^4.17.5",
    "lusca": "^1.5.2",
    "mongoose": "^5.0.10",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "nodemailer": "^4.6.3",
    "papaparse": "^4.3.7",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.1",
    "request": "^2.85.0",
    "rimraf": "^2.6.2",
    "tree-kill": "^1.2.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/async": "^2.0.47",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.2",
    "@types/compression": "0.0.33",
    "@types/connect-mongo": "0.0.34",
    "@types/dockerode": "^2.5.4",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/form-data": "^2.2.1",
    "@types/jasmine": "^2.8.6",
    "@types/jquery": "^3.3.1",
    "@types/json2csv": "^4.0.1",
    "@types/lodash": "^4.14.104",
    "@types/lusca": "^1.5.0",
    "@types/mongodb": "^2.2.20",
    "@types/mongoose": "^4.7.36",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.6",
    "@types/node": "^7.0.56",
    "@types/nodemailer": "^1.3.32",
    "@types/papaparse": "^4.1.33",
    "@types/passport": "^0.3.3",
    "@types/passport-local": "^1.0.32",
    "@types/request": "^2.47.0",
    "@types/rimraf": "^2.0.2",
    "@types/shelljs": "^0.7.8",
    "@types/supertest": "^2.0.4",
    "@types/uuid": "^3.4.3",
    "chai": "^4.1.2",
    "concurrently": "^3.5.1",
    "jest": "^21.2.1",
    "nodemon": "^1.17.2",
    "supertest": "^3.0.0",
    "ts-jest": "^21.2.4",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  }
}
